<div class="row"></div>
<div class="row">
    <div class="col s12 offset-m2 m8 offset-l3 l6">
        <ul class="tabs">
            <li class="tab">
                <a style="font-size:1.5em">
                    Rating List by Click Rate
                </a>
            </li>
        </ul>
    </div>
</div>
<div class="row">
    <table class="card-panel z-depth-1 hoverable col s12 offset-m2 m8 offset-l3 l6">
        <thead>
        </thead>
        <tbody>
            <% (0..@number-1).each do |counter| %>
            <tr>
                <% @feedlist[counter].id %>
                <td style="text-align:center; font-size:1.5em;">
                    <%= link_to @feedlist[counter].name, {:controller=>:feeds, :action=>:list, :address=>@feedlist[counter].source}, :class=>"black-text" %>
                </td>
                <td style="text-align:center">
                    <%= link_to '<i class="material-icons">add</i>'.html_safe,
                    subscriptions_add_path(:subscription=>{:feed_id=>@feedlist[counter].id}),
                    :class=>"btn-floating red",
                    data: { confirm: 'Are you sure?' } %>
                </td>
                <% @feedlist[counter].source %>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>
